ul {
    list-style-type: none;
    margin: 0;
    padding: 0;
    margin-bottom: 10px;
    float: left;
    background: #eee;
    padding: 5px;
    min-width: 170px;
    min-height: 30px;
    margin-right: 10px;
}
li {
    margin: 5px;
    padding: 5px;
    width: 150px;
}

.draggable, .formitem {
    border: 1px solid #FCEFA1;
    background: #FBF9EE;
    color: #363636;
}

#sortable li{
    width: 300px;
}